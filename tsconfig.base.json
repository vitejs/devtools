{
  "compilerOptions": {
    "composite": true,
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "rootDir": ".",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "paths": {
      "@vitejs/devtools-rpc/presets/ws/server": [
        "./packages/rpc/src/presets/ws/server.ts"
      ],
      "@vitejs/devtools-rpc/presets/ws/client": [
        "./packages/rpc/src/presets/ws/client.ts"
      ],
      "@vitejs/devtools-rpc": [
        "./packages/rpc/src"
      ],
      "@vitejs/devtools-kit/client": [
        "./packages/kit/src/client/index.ts"
      ],
      "@vitejs/devtools-kit/utils/events": [
        "./packages/kit/src/utils/events.ts"
      ],
      "@vitejs/devtools-kit/utils/nanoid": [
        "./packages/kit/src/utils/nanoid.ts"
      ],
      "@vitejs/devtools-kit/utils/shared-state": [
        "./packages/kit/src/utils/shared-state.ts"
      ],
      "@vitejs/devtools-kit": [
        "./packages/kit/src/index.ts"
      ],
      "@vitejs/devtools-vite": [
        "./packages/vite/src/index.ts"
      ],
      "@vitejs/devtools/client/inject": [
        "./packages/core/src/client/inject/index.ts"
      ],
      "@vitejs/devtools/client/webcomponents": [
        "./packages/core/src/client/webcomponents/index.ts"
      ],
      "@vitejs/devtools": [
        "./packages/core/src/index.ts"
      ]
    },
    "resolveJsonModule": true,
    "strict": true,
    "noEmit": true,
    "isolatedDeclarations": false,
    "skipLibCheck": true
  }
}
