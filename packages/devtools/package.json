{
  "name": "@vitejs/devtools",
  "type": "module",
  "version": "0.0.0-alpha.1",
  "description": "Vite DevTools",
  "author": "VoidZero Inc.",
  "license": "MIT",
  "homepage": "https://github.com/vitejs/devtools#readme",
  "repository": {
    "directory": "packages/devtools",
    "type": "git",
    "url": "git+https://github.com/vitejs/devtools.git"
  },
  "bugs": "https://github.com/vitejs/devtools/issues",
  "keywords": [
    "vite",
    "devtools",
    "rpc"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./webcomponents": "./dist/webcomponents/index.js",
    "./client-inject": "./dist/client-inject/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vite-devtools": "./bin.mjs"
  },
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepack": "pnpm build",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build"
  },
  "peerDependencies": {
    "vite": "catalog:build"
  },
  "dependencies": {
    "@vitejs/devtools-kit": "workspace:*",
    "@vitejs/devtools-rpc": "workspace:*",
    "ws": "catalog:deps"
  },
  "devDependencies": {
    "@vitejs/devtools": "workspace:*",
    "@vitejs/devtools-vite": "workspace:*",
    "tsdown": "catalog:build",
    "unplugin-vue": "catalog:build"
  }
}
